		testDataList.add(0);
		testDataList.add(1);
		testDataList.add(2);
		testDataList.add(3);


		int Actual_1=Sum_Pairs.findPairs(testDataList,testDataTarget);
		Assert.assertEquals("", 2, Actual_1);

	}
